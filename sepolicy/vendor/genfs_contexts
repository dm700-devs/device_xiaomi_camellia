# Audio
genfscon sysfs /devices/platform/11f00000.i2c6/i2c-6/6-0058                              u:object_r:mtk_audiohal_data_file:s0

# Fingerprint
genfscon sysfs /devices/platform/fingerprint                                             u:object_r:vendor_fingerprint_data_file:s0

# Wakeup (https://lkml.org/lkml/2019/8/6/1275)
genfscon sysfs /devices/platform/11e01000.i2c5/i2c-5/5-004e/tcpc/type_c_port0/wakeup     u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11e01000.i2c5/i2c-5/5-004e/wakeup                       u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11e02000.i2c7/i2c-7/7-0008/wakeup                       u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/virtual/input/input2/wakeup                                      u:object_r:sysfs_wakeup:s0
